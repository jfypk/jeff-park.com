<script>
    var menuOn = false;
    var menu = null;
    function menuClick(x) {
        x.classList.toggle("change");
        menuOn = !menuOn;
        menu = document.getElementById("menu");
        if(menuOn) {
            menu.style.display = "block";
        } else {
            menu.style.display = "none";
        }
        console.log(menuOn);
    }
</script>

